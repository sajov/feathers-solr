{
  "name": "feathers-solr",
  "description": "A service plugin for Solr",
  "version": "3.0.0",
  "keywords": [
    "feathers",
    "feathers-plugin",
    "solr",
    "solr-client"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/sajov/feathers-solr.git"
  },
  "author": {
    "name": "sajov"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/sajov/feathers-solr/issues"
  },
  "engines": {
    "node": ">= 16"
  },
  "main": "lib/",
  "types": "types",
  "scripts": {
