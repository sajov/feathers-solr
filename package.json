{
  "name": "feathers-solr",
  "description": "A service plugin for Solr",
  "version": "3.0.5",
  "keywords": [
    "feathers",
    "feathers-plugin",
    "solr",
    "solr-client",
    "search",
    "database",
    "api"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/sajov/feathers-solr.git"
  },
  "author": {
    "name": "sajov"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/sajov/feathers-solr/issues"
  },
  "engines": {
    "node": ">= 16"
  },
  "main": "lib/",
  "scripts": {
    "prepare": "npm run build",
    "prepublish": "npm run build",
    "lint": "eslint src/**/*.ts test/**/*.ts --fix",
    "build": "shx rm -rf lib/ && tsc",
    "test": "npm run build && npm run mocha",
    "mocha": "mocha --config .mocharc.json --recursive test/**.test.ts test/**/*.test.ts",
    "coverage": "nyc npm run test"
  },
  "directories": {
    "lib": "lib"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.0-pre.15",
    "@feathersjs/commons": "^5.0.0-pre.15",
    "@feathersjs/errors": "^5.0.0-pre.15"
  },
  "devDependencies": {
    "@feathersjs/adapter-tests": "^5.0.0-pre.15",
    "@feathersjs/express": "^5.0.0-pre.15",
    "@feathersjs/feathers": "^5.0.0-pre.15",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "shx": "^0.3.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
