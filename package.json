    "@feathersjs/errors": "^5.0.0-pre.14"
  },
  "devDependencies": {
    "@feathersjs/adapter-tests": "^5.0.0-pre.14",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "nodemon": "^2.0.15",
    "shx": "^0.3.3",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
